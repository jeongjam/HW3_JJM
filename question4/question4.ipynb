    "def fac(a):\n",
    "    if(a==0): return 1\n",
    "    return a*fac(a-1)\n",
    "\n",
    "def main():\n",
    "    for i in range(8):\n",
    "        print(\"{0:d}! : {1:d}\".format(2*i,fac(2*i)))\n",
    "\n",
    "if __name__==\"__main__\":\n",
    "    main()"
